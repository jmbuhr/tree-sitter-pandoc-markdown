{
  "grammars": [
    {
      "name": "@grammars/markdown",
      "camelcase": "GrammarsMarkdown",
      "scope": "text.markdown",
      "path": "tree-sitter-pandoc-markdown",
      "external-files": [
        "common/common.js"
      ],
      "file-types": [
        "md",
        "qmd",
        "rmd"
      ],
      "highlights": "tree-sitter-pandoc-markdown/queries/highlights.scm",
      "injections": "tree-sitter-pandoc-markdown/queries/injections.scm",
      "injection-regex": "^(markdown|md|qmd|rmd)$"
    },
    {
      "name": "@grammars/pandoc_markdown",
      "camelcase": "GrammarsPandocMarkdown",
      "scope": "text.pandoc_markdown_inline",
      "path": "tree-sitter-pandoc-markdown-inline",
      "external-files": [
        "common/common.js"
      ],
      "file-types": null,
      "highlights": "tree-sitter-pandoc-markdown-inline/queries/highlights.scm",
      "injections": "tree-sitter-pandoc-markdown-inline/queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Pandoc Markdown grammar for tree-sitter",
    "authors": [
      {
        "name": "Jannik Buhr",
        "url": "https://jmbuhr.de"
      }
    ],
    "links": {
      "repository": "https://github.com/jmbuhr/tree-sitter-pandoc-markdown"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
