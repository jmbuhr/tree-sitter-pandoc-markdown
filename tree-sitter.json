{
  "grammars": [
    {
      "name": "pandoc_markdown",
      "camelcase": "PandocMarkdown",
      "scope": "source.pandoc_markdown",
      "path": ".",
      "file-types": null,
      "injection-regex": "^pandoc_markdown$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Treesitter parser for markdown with pandoc extensions",
    "authors": [
      {
        "name": "Jannik Buhr",
        "email": "jannik.m.buhr@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/jmbuhr/tree-sitter-pandoc-markdown"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}